{
  "name": "react-tailwindcss",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "build:analyze": "npm run build && npx vite-bundle-analyzer",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "build:docker": "docker build -f Dockerfile.frontend -t todo-frontend .",
    "start:docker": "docker run -p 3000:80 todo-frontend",
    "deploy:prepare": "npm run lint:fix && npm run type-check && npm run build",
    "deploy:check": "npm run deploy:prepare && echo 'Ready for deployment!'",
    "postbuild": "echo 'Frontend build completed successfully!'"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.6",
    "@types/sql.js": "^1.4.9",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.7.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.548.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sql.js": "^1.13.0",
    "tailwindcss": "^4.1.6",
    "uuid": "^9.0.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
